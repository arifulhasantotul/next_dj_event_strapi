{
  "kind": "collectionType",
  "collectionName": "events",
  "info": {
    "singularName": "event",
    "pluralName": "events",
    "displayName": "Event",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "slug": {
      "type": "uid",
      "targetField": "name"
    },
    "venue": {
      "type": "string"
    },
    "address": {
      "type": "string"
    },
    "date": {
      "type": "datetime"
    },
    "time": {
      "type": "string"
    },
    "performers": {
      "type": "string"
    },
    "description": {
      "type": "richtext"
    },
    "image": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "users_permissions": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "plugin::users-permissions.user"
    }
